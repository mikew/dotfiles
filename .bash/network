alias server="npx serve -p 4000"
alias server-single="npx serve -p 4000 --single"

# All the dig info
function digga() {
	dig +nocmd "$1" any +multiline +noall +answer
}

ssh-tunnel-port () {
  local host="$1"
  local port="$2"

  ssh -NL "$port":0.0.0.0:"$port" "$host"
}

ssh-proxy () {
  local host="$1"
  ssh -D 8888 "$host"
}

# vim: ft=sh
